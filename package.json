{
  "name": "static-api-controller",
  "version": "0.5.0",
  "description": "CLI that will obtain the necessary data using webscraping or consuming other APIs, process it and send the information to the api repository",
  "main": "src/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "tsc-watch --onSuccess \"node dist/main.js\"",
    "build": "tsc"
  },
  "keywords": [
    "cli",
    "webscraping",
    "puppeteer",
    "typescript",
    "nodejs"
  ],
  "author": "erianvc",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "inquirer": "^7.3.3",
    "puppeteer-core": "^5.3.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/puppeteer-core": "^2.0.0",
    "@types/shelljs": "^0.8.8",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.2"
  }
}
