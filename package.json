{
  "name": "static-api-controller",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node build/src/main.js",
    "lint": "eslint --fix \"src/**/*.ts\"",
    "restart": "npm run build && npm start",
    "prebuild": "rimraf coverage build tmp",
    "prelint": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@octokit/rest": "18.12.0",
    "inquirer": "8.2.2",
    "node-fetch": "3.2.3",
    "playwright": "1.20.2",
    "shelljs": "0.8.5"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.1",
    "@types/node": "17.0.23",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "dotenv": "16.0.0",
    "eslint": "8.12.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "tsc-alias": "1.6.5",
    "typescript": "4.6.3"
  }
}
