{
  "name": "static-api-controller",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node build/src/main.js",
    "lint": "eslint --fix src/**/*.ts",
    "restart": "npm run build && npm start",
    "prebuild": "rimraf coverage build tmp",
    "prelint": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@octokit/rest": "18.12.0",
    "axios": "0.27.2",
    "inquirer": "8.2.2",
    "playwright": "1.24.0",
    "shelljs": "0.8.5"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.1",
    "@types/node": "17.0.23",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "dotenv": "16.0.1",
    "eslint": "8.20.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "tsc-alias": "1.6.5",
    "typescript": "4.7.4"
  }
}
